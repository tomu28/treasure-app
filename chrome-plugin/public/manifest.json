{
  "manifest_version": 2,
  "name": "Chrome Plugin",
  "description": "Get Title And URL of a Page.",
  "version": "0.0.1",
  "browser_action": {
    "default_title": "Title & URL Display",
    "default_popup": "./index.html",
    "default_icon": "favicon.ico"
  },
  "permissions": [
    "activeTab"
  ]
}
